{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport { Geolocation } from '@capacitor/geolocation';\nexport default {\n  name: 'DistanceForm',\n  data() {\n    return {\n      distance: '',\n      // Speichert die eingegebene Distanz\n      submitted: false,\n      // Gibt an, ob das Formular bereits abgesendet wurde\n      location: null // Speichert den aktuellen Standort\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      this.submitted = true;\n\n      // Wenn kein Standort abgerufen wurde, zeige eine Fehlermeldung\n      if (!this.location) {\n        alert('Standort konnte nicht abgerufen werden.');\n        return;\n      }\n      const data = {\n        distance: this.distance,\n        location: {\n          latitude: this.location.latitude,\n          longitude: this.location.longitude,\n          accuracy: this.location.accuracy\n        }\n      };\n\n      // Konvertiere das Datenobjekt in JSON\n      const jsonData = JSON.stringify(data, null, 2);\n\n      // Erstelle einen Blob, der das JSON enthält\n      const blob = new Blob([jsonData], {\n        type: 'application/json'\n      });\n\n      // Erstelle einen temporären Link, um die Datei herunterzuladen\n      const a = document.createElement('a');\n      a.href = URL.createObjectURL(blob);\n      a.download = 'data.json'; // Die Datei wird als data.json gespeichert\n      a.click();\n      URL.revokeObjectURL(a.href);\n      console.log('Daten in data.json gespeichert:', data);\n    },\n    async getLocation() {\n      try {\n        // Abrufen des Standorts mit hoher Genauigkeit\n        const position = await Geolocation.getCurrentPosition({\n          enableHighAccuracy: true\n        });\n\n        // Überprüfen, ob die Koordinaten korrekt sind\n        if (position.coords) {\n          this.location = position.coords;\n          console.log('Standort abgerufen:', this.location);\n        } else {\n          console.error('Koordinaten konnten nicht abgerufen werden.');\n        }\n      } catch (error) {\n        // Fehlerbehandlung bei fehlgeschlagener Standortabfrage\n        console.error('Fehler beim Abrufen des Standorts:', error);\n        alert('Fehler beim Abrufen des Standorts.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["Geolocation","name","data","distance","submitted","location","methods","handleSubmit","alert","latitude","longitude","accuracy","jsonData","JSON","stringify","blob","Blob","type","a","document","createElement","href","URL","createObjectURL","download","click","revokeObjectURL","console","log","getLocation","position","getCurrentPosition","enableHighAccuracy","coords","error"],"sources":["C:\\Users\\madiw\\Desktop\\Hochschule\\7. Semester\\Bachelorprojekt\\projekt\\src\\components\\testapp.vue"],"sourcesContent":["<template>\r\n  <div class=\"form-container\">\r\n    <h1>Distanz zum Baum und Standort</h1>\r\n\r\n    <form @submit.prevent=\"handleSubmit\">\r\n      <div class=\"form-group\">\r\n        <label for=\"distance\">Distanz zum Baum</label>\r\n        <input\r\n          type=\"number\"\r\n          id=\"distance\"\r\n          v-model=\"distance\"\r\n          required\r\n          min=\"0\"\r\n          step=\"0.1\"\r\n          placeholder=\"z. B. 25.5\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <button type=\"submit\" class=\"submit-button\">Absenden</button>\r\n      </div>\r\n    </form>\r\n\r\n    <div class=\"form-group\">\r\n      <button @click=\"getLocation\" class=\"location-button\">Standort abrufen</button>\r\n    </div>\r\n\r\n    <!-- Anzeige des Standorts -->\r\n    <div v-if=\"location\">\r\n      <p>Aktueller Standort: {{ location.latitude }}°, {{ location.longitude }}°</p>\r\n      <p>Genauigkeit: {{ location.accuracy }} Meter</p>\r\n    </div>\r\n\r\n    <!-- Anzeige der eingegebenen Distanz -->\r\n    <div v-if=\"submitted\">\r\n      <p>Die eingegebene Distanz zum Baum beträgt: <strong>{{ distance }} Meter</strong></p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Geolocation } from '@capacitor/geolocation';\r\n\r\nexport default {\r\n  name: 'DistanceForm',\r\n  data() {\r\n    return {\r\n      distance: '', // Speichert die eingegebene Distanz\r\n      submitted: false, // Gibt an, ob das Formular bereits abgesendet wurde\r\n      location: null, // Speichert den aktuellen Standort\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSubmit() {\r\n      this.submitted = true;\r\n      \r\n      // Wenn kein Standort abgerufen wurde, zeige eine Fehlermeldung\r\n      if (!this.location) {\r\n        alert('Standort konnte nicht abgerufen werden.');\r\n        return;\r\n      }\r\n\r\n      const data = {\r\n        distance: this.distance,\r\n        location: {\r\n          latitude: this.location.latitude,\r\n          longitude: this.location.longitude,\r\n          accuracy: this.location.accuracy,\r\n        },\r\n      };\r\n\r\n      // Konvertiere das Datenobjekt in JSON\r\n      const jsonData = JSON.stringify(data, null, 2);\r\n\r\n      // Erstelle einen Blob, der das JSON enthält\r\n      const blob = new Blob([jsonData], { type: 'application/json' });\r\n\r\n      // Erstelle einen temporären Link, um die Datei herunterzuladen\r\n      const a = document.createElement('a');\r\n      a.href = URL.createObjectURL(blob);\r\n      a.download = 'data.json';  // Die Datei wird als data.json gespeichert\r\n      a.click();\r\n      URL.revokeObjectURL(a.href);\r\n\r\n      console.log('Daten in data.json gespeichert:', data);\r\n    },\r\n\r\n    async getLocation() {\r\n      try {\r\n        // Abrufen des Standorts mit hoher Genauigkeit\r\n        const position = await Geolocation.getCurrentPosition({ enableHighAccuracy: true });\r\n        \r\n        // Überprüfen, ob die Koordinaten korrekt sind\r\n        if (position.coords) {\r\n          this.location = position.coords;\r\n          console.log('Standort abgerufen:', this.location);\r\n        } else {\r\n          console.error('Koordinaten konnten nicht abgerufen werden.');\r\n        }\r\n      } catch (error) {\r\n        // Fehlerbehandlung bei fehlgeschlagener Standortabfrage\r\n        console.error('Fehler beim Abrufen des Standorts:', error);\r\n        alert('Fehler beim Abrufen des Standorts.');\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Grundlegendes Styling für das Formular */\r\n.form-container {\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 8px;\r\n  background-color: #f9f9f9;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n  font-size: 16px;\r\n  color: #333;\r\n  margin-bottom: 5px;\r\n  display: block;\r\n}\r\n\r\ninput {\r\n  width: 100%;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n}\r\n\r\nbutton.submit-button, .location-button {\r\n  width: 100%;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  background-color: #4CAF50;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  margin-top: 10px;\r\n}\r\n\r\nbutton.submit-button:hover, .location-button:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\ndiv p {\r\n  text-align: center;\r\n  font-size: 18px;\r\n  color: #333;\r\n}\r\n</style>\r\n"],"mappings":";;;AAyCA,SAASA,WAAU,QAAS,wBAAwB;AAEpD,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MAAE;MACdC,SAAS,EAAE,KAAK;MAAE;MAClBC,QAAQ,EAAE,IAAI,CAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI,CAACH,SAAQ,GAAI,IAAI;;MAErB;MACA,IAAI,CAAC,IAAI,CAACC,QAAQ,EAAE;QAClBG,KAAK,CAAC,yCAAyC,CAAC;QAChD;MACF;MAEA,MAAMN,IAAG,GAAI;QACXC,QAAQ,EAAE,IAAI,