{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport { Geolocation } from '@capacitor/geolocation';\nexport default {\n  name: 'DistanceForm',\n  data() {\n    return {\n      distance: '',\n      submitted: false,\n      location: null\n    };\n  },\n  computed: {\n    iframeUrl() {\n      //if (!this.location) return '';\n      const {\n        latitude,\n        longitude\n      } = this.location;\n      return `https://www.openstreetmap.org/export/embed.html?bbox=${longitude}%2C${latitude}%2C${longitude}%2C${latitude}&layer=mapnik&marker=${latitude}%2C${longitude}`;\n    },\n    mapLink() {\n      //if (!this.location) return '';\n      const {\n        latitude,\n        longitude\n      } = this.location;\n      return `https://www.openstreetmap.org/?mlat=${latitude}&mlon=${longitude}#map=12/${latitude}/${longitude}`;\n    }\n  },\n  methods: {\n    async handleSubmit() {\n      this.submitted = true;\n      if (!this.location) {\n        alert('Standort konnte nicht abgerufen werden.');\n        return;\n      }\n      const data = {\n        distance: this.distance,\n        location: {\n          latitude: this.location.latitude,\n          longitude: this.location.longitude,\n          accuracy: this.location.accuracy\n        }\n      };\n      const jsonData = JSON.stringify(data, null, 2);\n      const blob = new Blob([jsonData], {\n        type: 'application/json'\n      });\n      const a = document.createElement('a');\n      a.href = URL.createObjectURL(blob);\n      a.download = 'data.json';\n      a.click();\n      URL.revokeObjectURL(a.href);\n      console.log('Daten in data.json gespeichert:', data);\n    },\n    async getLocation() {\n      try {\n        const position = await Geolocation.getCurrentPosition({\n          enableHighAccuracy: true\n        });\n        if (position.coords) {\n          this.location = position.coords;\n          console.log('Standort abgerufen:', this.location);\n        } else {\n          console.error('Koordinaten konnten nicht abgerufen werden.');\n        }\n      } catch (error) {\n        console.error('Fehler beim Abrufen des Standorts:', error);\n        alert('Fehler beim Abrufen des Standorts.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["Geolocation","name","data","distance","submitted","location","computed","iframeUrl","latitude","longitude","mapLink","methods","handleSubmit","alert","accuracy","jsonData","JSON","stringify","blob","Blob","type","a","document","createElement","href","URL","createObjectURL","download","click","revokeObjectURL","console","log","getLocation","position","getCurrentPosition","enableHighAccuracy","coords","error"],"sources":["C:\\Users\\madiw\\Desktop\\Hochschule\\7. Semester\\Bachelorprojekt\\projekt\\src\\components\\testapp.vue"],"sourcesContent":["<template>\r\n  <div class=\"form-container\">\r\n    <h1>Distanz zum Baum und Standort</h1>\r\n\r\n    <form @submit.prevent=\"handleSubmit\">\r\n      <div class=\"form-group\">\r\n        <label for=\"distance\">Distanz zum Baum</label>\r\n        <input\r\n          type=\"number\"\r\n          id=\"distance\"\r\n          v-model=\"distance\"\r\n          required\r\n          min=\"0\"\r\n          step=\"0.1\"\r\n          placeholder=\"z. B. 5\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <button type=\"submit\" class=\"submit-button\">Absenden</button>\r\n      </div>\r\n    </form>\r\n\r\n    <div class=\"form-group\">\r\n      <button @click=\"getLocation\" class=\"location-button\">Standort abrufen</button>\r\n    </div>\r\n\r\n    <div v-if=\"location\">\r\n      <p>Aktueller Standort: {{ location.latitude }}°, {{ location.longitude }}°</p>\r\n      <p>Genauigkeit: {{ location.accuracy }} Meter</p>\r\n    </div>\r\n\r\n    <div v-if=\"submitted\">\r\n      <p>Die eingegebene Distanz zum Baum beträgt: <strong>{{ distance }} Meter</strong></p>\r\n    </div>\r\n\r\n<!-- Anzeige der OpenStreetMap-Karte -->\r\n<div class=\"map-container\" v-if=\"location\">\r\n  <h2>Karte mit Standort</h2>\r\n  <iframe\r\n    :src=\"iframeUrl\"\r\n    width=\"425\"\r\n    height=\"350\"\r\n    style=\"border: 1px solid black\"\r\n  ></iframe>\r\n  <br />\r\n  <small>\r\n    <a :href=\"mapLink\" target=\"_blank\" rel=\"noopener noreferrer\">\r\n      Größere Karte anzeigen\r\n    </a>\r\n  </small>\r\n</div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Geolocation } from '@capacitor/geolocation';\r\n\r\nexport default {\r\n  name: 'DistanceForm',\r\n  data() {\r\n    return {\r\n      distance: '', \r\n      submitted: false, \r\n      location: null, \r\n    };\r\n  },\r\n  computed: {\r\n  iframeUrl() {\r\n    //if (!this.location) return '';\r\n    const { latitude, longitude } = this.location;\r\n    return `https://www.openstreetmap.org/export/embed.html?bbox=${longitude}%2C${latitude}%2C${longitude}%2C${latitude}&layer=mapnik&marker=${latitude}%2C${longitude}`;\r\n  },\r\n  mapLink() {\r\n    //if (!this.location) return '';\r\n    const { latitude, longitude } = this.location;\r\n    return `https://www.openstreetmap.org/?mlat=${latitude}&mlon=${longitude}#map=12/${latitude}/${longitude}`;\r\n  },\r\n},\r\n  methods: {\r\n    async handleSubmit() {\r\n      this.submitted = true;\r\n\r\n      if (!this.location) {\r\n        alert('Standort konnte nicht abgerufen werden.');\r\n        return;\r\n      }\r\n\r\n      const data = {\r\n        distance: this.distance,\r\n        location: {\r\n          latitude: this.location.latitude,\r\n          longitude: this.location.longitude,\r\n          accuracy: this.location.accuracy,\r\n        },\r\n      };\r\n\r\n      const jsonData = JSON.stringify(data, null, 2);\r\n\r\n      const blob = new Blob([jsonData], { type: 'application/json' });\r\n\r\n      const a = document.createElement('a');\r\n      a.href = URL.createObjectURL(blob);\r\n      a.download = 'data.json'; \r\n      a.click();\r\n      URL.revokeObjectURL(a.href);\r\n\r\n      console.log('Daten in data.json gespeichert:', data);\r\n    },\r\n\r\n    async getLocation() {\r\n      try {\r\n\r\n        const position = await Geolocation.getCurrentPosition({ enableHighAccuracy: true });\r\n\r\n        if (position.coords) {\r\n          this.location = position.coords;\r\n          console.log('Standort abgerufen:', this.location);\r\n        } else {\r\n          console.error('Koordinaten konnten nicht abgerufen werden.');\r\n        }\r\n      } catch (error) {\r\n\r\n        console.error('Fehler beim Abrufen des Standorts:', error);\r\n        alert('Fehler beim Abrufen des Standorts.');\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.form-container {\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 8px;\r\n  background-color: #f9f9f9;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n  font-size: 16px;\r\n  color: #333;\r\n  margin-bottom: 5px;\r\n  display: block;\r\n}\r\n\r\ninput {\r\n  width: 100%;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n}\r\n\r\nbutton.submit-button,\r\n.location-button {\r\n  width: 100%;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  background-color: #4CAF50;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  margin-top: 10px;\r\n}\r\n\r\nbutton.submit-button:hover,\r\n.location-button:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\ndiv p {\r\n  text-align: center;\r\n  font-size: 18px;\r\n  color: #333;\r\n}\r\n\r\n.map-container {\r\n  margin-top: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.map-container h2 {\r\n  font-size: 20px;\r\n  color: #333;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.map-image {\r\n  width: 100%;\r\n  max-width: 450px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n}\r\n</style>\r\n"],"mappings":";;;AAyDA,SAASA,WAAU,QAAS,wBAAwB;AAEpD,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,KAAK;MAChBC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACVC,SAASA,CAAA,EAAG;MACV;MACA,MAAM;QAAEC,QAAQ;QAAEC;MAAU,IAAI,IAAI,CAACJ,QAAQ;MAC7C,OAAO,wDAAwDI,SAAS,MAAMD,QAAQ,MAAMC,SAAS,MAAMD,QAAQ,wBAAwBA,QAAQ,MAAMC,SAAS,EAAE;IACtK,CAAC;IACDC,OAAOA,CAAA,EAAG;MACR;MACA,MAAM;QAAEF,QAAQ;QAAEC;MAAU,IAAI,IAAI,CAACJ,QAAQ;MAC7C,OAAO,uCAAuCG,QAAQ,SAASC,SAAS,WAAWD,QAAQ,IAAIC,SAAS,EAAE;IAC5G;EACF,CAAC;EACCE,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI,CAACR,SAAQ,GAAI,IAAI;MAErB,IAAI,CAAC,IAAI,CAACC,QAAQ,EAAE;QAClBQ,KAAK,CAAC,yCAAyC,CAAC;QAChD;MACF;MAEA,MAAMX,IAAG,GAAI;QACXC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBE,QAAQ,EAAE;UACRG,QAAQ,EAAE,IAAI,CAACH,QAAQ,CAACG,QAAQ;UAChCC,SAAS,EAAE,IAAI,CAACJ,QAAQ,CAACI,SAAS;UAClCK,QAAQ,EAAE,IAAI,CAACT,QAAQ,CAACS;QAC1B;MACF,CAAC;MAED,MAAMC,QAAO,GAAIC,IAAI,CAACC,SAAS,CAACf,IAAI,EAAE,IAAI,EAAE,CAAC,CAAC;MAE9C,MAAMgB,IAAG,GAAI,IAAIC,IAAI,CAAC,CAACJ,QAAQ,CAAC,EAAE;QAAEK,IAAI,EAAE;MAAmB,CAAC,CAAC;MAE/D,MAAMC,CAAA,GAAIC,QAAQ,CAACC,aAAa,CAAC,GAAG,CAAC;MACrCF,CAAC,CAACG,IAAG,GAAIC,GAAG,CAACC,eAAe,CAACR,IAAI,CAAC;MAClCG,CAAC,CAACM,QAAO,GAAI,WAAW;MACxBN,CAAC,CAACO,KAAK,CAAC,CAAC;MACTH,GAAG,CAACI,eAAe,CAACR,CAAC,CAACG,IAAI,CAAC;MAE3BM,OAAO,CAACC,GAAG,CAAC,iCAAiC,EAAE7B,IAAI,CAAC;IACtD,CAAC;IAED,MAAM8B,WAAWA,CAAA,EAAG;MAClB,IAAI;QAEF,MAAMC,QAAO,GAAI,MAAMjC,WAAW,CAACkC,kBAAkB,CAAC;UAAEC,kBAAkB,EAAE;QAAK,CAAC,CAAC;QAEnF,IAAIF,QAAQ,CAACG,MAAM,EAAE;UACnB,IAAI,CAAC/B,QAAO,GAAI4B,QAAQ,CAACG,MAAM;UAC/BN,OAAO,CAACC,GAAG,CAAC,qBAAqB,EAAE,IAAI,CAAC1B,QAAQ,CAAC;QACnD,OAAO;UACLyB,OAAO,CAACO,KAAK,CAAC,6CAA6C,CAAC;QAC9D;MACF,EAAE,OAAOA,KAAK,EAAE;QAEdP,OAAO,CAACO,KAAK,CAAC,oCAAoC,EAAEA,KAAK,CAAC;QAC1DxB,KAAK,CAAC,oCAAoC,CAAC;MAC7C;IACF;EACF;AACF,CAAC","ignoreList":[]},